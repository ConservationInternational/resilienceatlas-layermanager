{
  "name": "resilience-layer-manager",
  "private": true,
  "version": "1.0.6",
  "description": "A library to get a layer depending on provider and layer spec",
  "repository": {
    "type": "git",
    "url": "https://github.com/ConservationInternational/resilienceatlas-layermanager"
  },
  "author": "Vizzuality",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "dist/layer-manager.esm.js",
  "exports": {
    ".": {
      "import": "./dist/layer-manager.esm.js",
      "require": "./lib/index.js"
    },
    "./components": {
      "import": "./dist/components/index.esm.js",
      "require": "./lib/components/index.js"
    }
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "serve": "http-server . -p 8000 -a 0.0.0.0",
    "prepublishOnly": "npm run build",
    "lint": "eslint ./src",
    "test": "mocha --require @babel/register --require ./test/setup --recursive ./test/specs",
    "watch": "npm run rollup:watch & npm run babel:watch",
    "build": "npm run rollup:build && npm run babel:build",
    "rollup:build": "cross-env NODE_ENV=production rollup -c",
    "rollup:watch": "cross-env BABEL_ENV=rollup rollup -c -w",
    "babel:build": "babel src -d lib",
    "babel:watch": "babel src -d lib --watch",
    "prettier": "prettier --single-quote --write \"src/**/*.js\""
  },
  "devDependencies": {
    "@babel/cli": "^7.26.0",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-class-properties": "^7.25.0",
    "@babel/plugin-transform-object-rest-spread": "^7.25.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.0",
    "@babel/register": "^7.25.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "chai": "^5.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.0",
    "jsdom": "^26.0.0",
    "mocha": "^11.0.0",
    "prettier": "^3.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.28.0"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "supercluster": "^8.0.1",
    "wri-json-api-serializer": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
